<h1>タスク一覧</h1>
<ul>
    <% @tasks.each do |task| %>
    <li>
        <%= link_to task.id, task_path(task) %>
        <%= link_to task.task, task_path(task) %>
        <%= link_to task.description, task_path(task) %>
        <%= link_to task.status, task_path(task) %>
    </li>
    <% end %>
</ul>
<%= link_to '新規タスクの入力', new_task_path %>